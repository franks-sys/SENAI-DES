generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model carros {
  id Int @id @default(autoincrement())
  placa String@unique
  marca String
  modelo String
  ano Int

}

model clientes{
  id Int @id @default(autoincrement())
  nome String 
  CPF Int
  email String@unique
  CNH String
}